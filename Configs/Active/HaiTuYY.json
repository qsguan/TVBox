{
    "author": "Jimmy_Bytheway",
    "ua": "Mozilla/5.0 (iPhone; CPU iPhone OS 13_2_3 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/13.0.3 Mobile/15E148 Safari/604.1",
    "homeUrl":   "https://haituu.tv/",
    "dcPlayUrl": "true",
    "dcVipFlag": "true",
    "cateNode":  "//div[@class='nav']/ul[@class='nav-menu-items']/li/a[contains(@href,'type')]",
    "cateName":  "/@title",
    "cateId":    "/@href",
    "cateIdR":   "/vod/type/id/(\\S+).html",
    "cateManual": {
        "电影库": "1",
        "电视剧": "2",
        "综艺库": "3",
        "动漫库": "4",
        "纪录片1": "20"
    },
    
// Home Summary ================================================
    "homeVodNode":  "//div[@class='content']/div[contains(@class,'module')][position()<6]//div[@class='module-item']",
    "homeVodName":  "/div[contains(@class,'box')]/a/@title",
    "homeVodId":    "/div[contains(@class,'box')]/a/@href",
    "homeVodIdR":   "/vod/detail/id/(\\S+).html",
    "homeVodImg":   "/div[contains(@class,'cover')]/div/img/@data-src",
    "homeVodImgR":  ".*(http\\S+)",
    "homeVodMark":  "concat(//div[contains(@class,'caption')]/span[1]/text(),'/',/div[contains(@class,'text')]/text())",
    
// Category Summary ================================================
                   //https://haituu.tv/vod/show/area/大陆/by/score/class/剧情/id/11/lang/国语/page/2/year/2022.html
    "cateUrl":      "https://haituu.tv/vod/show{area}{by}{class}/id/{cateId}{lang}/page/{catePg}{year}.html",
    "cateVodNode":  "//div[@class='content']/div[contains(@class,'module')]//div[@class='module-item']",
    "cateVodName":  "/div[contains(@class,'box')]/a/@title",
    "cateVodId":    "/div[contains(@class,'box')]/a/@href",
    "cateVodIdR":   "/vod/detail/id/(\\S+).html",
    "cateVodImg":   "/div[contains(@class,'cover')]/div/img/@data-src",
    "cateVodImgR":  ".*(http\\S+)",
    "cateVodMark":  "concat(//div[contains(@class,'caption')]/span[1]/text(),'/',/div[contains(@class,'text')]/text())",
    
// Video Details ================================================
    "dtUrl":        "https://haituu.tv/vod/detail/id/{vid}.html",
    "dtNode":       "//div[@class='box view-heading']",
    "dtName":       "//h1[@class='page-title']/text()",
    "dtImg":        "//div[@class='video-cover']/div/div/img/@data-src",
    "dtImgR":       ".*(http\\S+)",
    "dtCate":       "concat(//div[contains(@class,'video-info-aux')]/div/a/text())",
    "dtYear":       "//div[contains(@class,'video-info-aux')]/a[2]/text()",
    "dtArea":       "//div[contains(@class,'video-info-aux')]/a[3]/text()",
    "dtMark":       "//span[contains(text(),'集数')]/following-sibling::div/text()",
    "dtDirector":   "concat(//span[contains(text(),'导演')]/following-sibling::div/a[position()<6]/text())",
    "dtActor":      "concat(//span[contains(text(),'主演')]/following-sibling::div/a[position()<6]/text())",
    "dtDesc":       "//span[contains(text(),'剧情')]/following-sibling::div/span/text()",
    
// Video Playlist =====================================================
    "dtFromNode":   "//div[contains(@class,'module-player-tab')]/div/div/div/span",
    #"dtFromName":   "/text()",
    "dtFromName":   "concat('播放源',count(parent::*/parent::*/parent::*/preceding::*/div[contains(@class,'module-player-tab')]/div/div/div/span)+1)",
    "dtUrlNode":    "//div[contains(@class,'module-player-list')]/div[contains(@class,'box')]/div",
    "dtUrlSubNode": "/a",
    "dtUrlId":      "/@href",
    "dtUrlIdR":     "/vod/play/id/(\\S+).html",
    "dtUrlName":    "/span/text()",
    "playUrl":      "https://haituu.tv/vod/play/id/{playUrl}.html",
    "playUa":       "",
    
// Search Results ================================================
    "searchUrl":    "https://haituu.tv/vod/search.html?wd={wd}",
    "scVodNode":    "//div[@class='content']/div[contains(@class,'module')]//div[@class='module-search-item']",
    "scVodName":    "//div[contains(@class,'info-header')]/a/@title",
    "scVodId":      "//div[contains(@class,'info-header')]/a/@href",
    "scVodIdR":     "/vod/detail/id/(\\S+).html",
    "scVodImg":     "//div[contains(@class,'cover')]/div/img/@data-src",
    "scVodImgR":    ".*(http\\S+)",
    "scVodMark":    "concat(//div[contains(@class,'video-info-aux')]/div[1]//a/text(),'/',//div[@class='video-info-header']/a/text())",
    
// Filters =======================================================
    "filter": {
        "1": [

        ],
        "2": [

        ],
        "3": [

        ],
        "4": [

        ],
        "20": [

        ]
    }
}
