{
    "author": "Jimmy_Bytheway",
    "ua": "Mozilla/5.0 (iPhone; CPU iPhone OS 13_2_3 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/13.0.3 Mobile/15E148 Safari/604.1",
    "homeUrl":   "https://gimy.video/",
    "dcPlayUrl": "true",
    "dcVipFlag": "true",
    "cateNode":  "//ul[contains(@class,'navbar-items')]/li/a[contains(@href,'channel')]",
    "cateName":  "/@title",
    "cateId":    "/@href",
    "cateIdR":   "/channel/(\\w+).html",
    "cateManual": {
        "电影库": "1",
        "电视剧": "2",
        "综艺库": "29",
        "动漫库": "30",
        "纪录片2": "22"
    },
    
// Home Summary ================================================
    "homeVodNode":  "//div[contains(@class,'scroll-box') or contains(@class,'active')]/div/a",
    "homeVodName":  "/@title",
    "homeVodId":    "/@href",
    "homeVodIdR":   "/(\\S+).html",
    "homeVodImg":   "//div[contains(@class,'item-pic')]/img/@data-original",
    "homeVodMark":  "//div[contains(@class,'item-note')]/text()",
    
// Category Summary ================================================
                   //https://gimy.video/genre/11-美國-2022-2/by/hits_month/class/劇情.html
    "cateUrl":      "https://gimy.video/genre/{cateId}-{area}-{year}-{catePg}{by}{class}.html",
    "cateVodNode":  "//div[contains(@class,'scroll-box')]/a",
    "cateVodName":  "/@title",
    "cateVodId":    "/@href",
    "cateVodIdR":   "/(\\S+).html",
    "cateVodImg":   "//div[contains(@class,'item-pic')]/img/@data-original",
    "cateVodMark":  "//div[contains(@class,'item-note')]/text()",
    
// Video Details ================================================
    "dtUrl":        "https://gimy.video/{vid}.html",
    "dtNode":       "//div[@class='module-main']",
    "dtName":       "//div[contains(@class,'info-heading')]/h1/text()",
    "dtImg":        "//div[contains(@class,'item-pic')]/img/@data-original",
    "dtCate":       "//div[contains(@class,'info-heading')]/div/div[1]//text()",
    "dtYear":       "//div[contains(@class,'info-heading')]/div/div[2]//text()",
    "dtArea":       "//div[contains(@class,'info-heading')]/div/div[3]//text()",
    "dtMark":       "//span[contains(text(),'狀態')]/following-sibling::div/text()",
    "dtDirector":   "concat(//span[contains(text(),'導演')]/following-sibling::div/a[position()<6]/text())",
    "dtActor":      "concat(//span[contains(text(),'主演')]/following-sibling::div/a[position()<6]/text())",
    "dtDesc":       "//div[contains(@class,'module-info-introduction-content')]",
    
// Video Playlist =====================================================
    "dtFromNode":   "//div[contains(@class,'player-heading')]/div/div/div/div",
    "dtFromName":   "/span/text()",
    "dtFromNameR":   "(\\S+線)",
    "dtUrlNode":    "//div[@class='module-play-list']/div[contains(@class,'module-play-list-content')]",
    "dtUrlSubNode": "/a",
    "dtUrlId":      "/@href",
    "dtUrlIdR":     "/(\\S+).html",
    "dtUrlName":    "/span/text()",
    "playUrl":      "https://gimy.video/{playUrl}.html",
    "playUa":       "",
    
// Search Results ================================================
    "searchUrl":    "https://gimy.video/search/-------------.html?wd={wd}",
    "scVodNode":    "//div[@class='module-card-item module-item']",
    "scVodName":    "//div[contains(@class,'item-title')]/a//text()",
    "scVodId":      "//div[contains(@class,'item-title')]/a/@href",
    "scVodIdR":     "/(\\S+).html",
    "scVodImg":     "/a/div/div/img/@data-original",
    "scVodMark":    "concat(/div[contains(@class,'item-class')]/text(),'/',//div[contains(@class,'item-note')]/text())",
    
// Filters =======================================================
    "filter": {
        "1": [

        ],
        "2": [

        ],
        "29": [

        ],
        "30": [

        ],
        "22": [

        ]
    }
}
