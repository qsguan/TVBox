{
    "author": "Jimmy_Bytheway",
    "ua": "Mozilla/5.0 (iPhone; CPU iPhone OS 13_2_3 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/13.0.3 Mobile/15E148 Safari/604.1",
    "homeUrl":   "https://www.555dyy.com/",
    "dcPlayUrl": "true",
    "dcVipFlag": "true",
    "cateNode":  "//ul[contains(@class,'navbar-items')]/li/a[contains(@href,'vodtype')]",
    "cateName":  "/@title",
    "cateId":    "/@href",
    "cateIdR":   "/vodtype/(\\S+)/",
    "cateManual": {
        "电影":   "dianying",
        "电视剧": "dianshiju",
        "综艺":   "zongyi",
        "动漫":   "dongman",
        "纪录片": "jilupian"
    },
    
// Home Summary ================================================
    "homeVodNode":  "//div[@class='module' and position()>1 and position()<7]//div[contains(@class,'module-items')]/a",
    "homeVodName":  "/@title",
    "homeVodId":    "/@href",
    "homeVodIdR":   "/voddetail/(\\S+)/",
    "homeVodImg":   "/div/div/img/@data-original",
    "homeVodMark":  "//div[@class='module-item-note']/text()",
    
// Category Summary ================================================
    "cateUrl":      "https://www.555dyy.com/vodshow/{cateId}-{area}-{by}-{catelog}-{lang}----{catePg}---{year}/",
    "cateVodNode":  "//div[@class='module']/a",
    "cateVodName":  "/@title",
    "cateVodId":    "/@href",
    "cateVodIdR":   "/voddetail/(\\S+)/",
    "cateVodImg":   "/div/div/img/@data-original",
    "cateVodMark":  "//div[@class='module-item-note']/text()",
    
// Video Details ================================================
    "dtUrl":        "https://www.555dyy.com/voddetail/{vid}/",
    "dtNode":       "//div[@class='module module-info']",
    "dtName":       "//div[@class='module-info-heading']/h1/text()",
    "dtImg":        "/div/div/div/div/img/@data-original",
    "dtCate":       "//div[@class='module-info-tag']/div[3]/a/text()",
    "dtYear":       "//div[@class='module-info-tag']/div[1]/a/text()",
    "dtArea":       "//div[@class='module-info-tag']/div[2]/a/text()",
    "dtMark":       "//span[contains(text(),'集数') or contains(text(),'备注') or contains(text(),'片长')]/following-sibling::*/text()",
    "dtDirector":   "concat(//span[contains(text(),'导演')]/following-sibling::*/a[position()<6]/text())",
    "dtActor":      "concat(//span[contains(text(),'主演')]/following-sibling::*/a[position()<6]/text())",
    "dtDesc":       "//div[contains(@class,'module-info-introduction-content')]//text()",
    
// Video Playlist =====================================================
    "dtFromNode":   "//div[contains(@class,'module-tab-items-box')]//div[contains(@class,'module-tab-item')]",
    "dtFromName":   "/span/text()",
    "dtUrlNode":    "//div[contains(@class,'module-play-list-content')]",
    "dtUrlSubNode": "/a",
    "dtUrlId":      "/@href",
    "dtUrlIdR":     "/vodplay/(\\S+)/",
    "dtUrlName":    "/span/text()",
    "playUrl":      "https://www.555dyy.com/vodplay/{playUrl}/",
    "playUa":       "",
    
// Search Results ================================================
    "searchUrl":    "https://www.555dyy.com/vodsearch/-------------/?wd={wd}",
    "scVodNode":    "//div[contains(@class,'module-items')]/div/a",
    "scVodName":    "/div/div/img/@alt",
    "scVodId":      "/@href",
    "scVodIdR":     "/voddetail/(\\S+)/",
    "scVodImg":     "/div/div/img/@data-original",
    "scVodMark":    "/div/div[@class='module-item-note']/text()",
    
// Filters =======================================================
    "filter": {
        "dianying": [

        ],
